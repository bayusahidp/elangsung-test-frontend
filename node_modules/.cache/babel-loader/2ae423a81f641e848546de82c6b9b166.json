kBAC;AACAV,gBADA;AAEAW,oBAFA;AAGAC,iBAHA;AAIAC,oBAJA;AAKAC,mBALA;AAMAC;AANA,OAlBA;AA0BAC;AACAhB,gBADA;AAEAW,oBAFA;AAGAC,iBAHA;AAIAC,oBAJA;AAKAC,mBALA;AAMAC;AANA;AA1BA;AAmCA,GA9DA;AA+DAE,SA/DA,qBA+DA;AACA;AACA,GAjEA;AAkEAC;AACAC,YADA,oBACAC,IADA,EACA;AACA;AACAC;AACArB,wBADA;AAEAsB;AAAAC;AAAA;AAFA;AAIA,KAPA;AAQAC,cARA,sBAQAJ,IARA,EAQA;AAAA;;AACA;AACA;AACA;AACA5B;AACAC,0BADA;AAEAC,iEAFA;AAGAC;AACA;AADA;AAHA,WAMAC,IANA,CAMA;AACA;AACA;AACAG;AACA0B;AACA,aAHA,MAIA;AACA1B;AACA;AACA;AACA,SAhBA,EAgBAD,KAhBA,CAgBA;AACAC;AACA;;AACA;AACAC;AADA;AAGA,SAtBA;AAuBA;AACA;AApCA;AAlEA","names":["components","Navbar","mounted","axios","method","url","headers","then","console","catch","alert","name","data","dialog","dialogDelete","align","sortable","text","value","users","editedIndex","editedItem","username","email","fakultas","jurusan","alamat","defaultItem","created","methods","editItem","item","self","params","id","deleteItem","location"],"sourceRoot":"src/views","sources":["Dashboard.vue"],"sourcesContent":["<template>\n<div>\n  <navbar/>\n  <v-container>\n    <v-data-table\n    :headers=\"headers\"\n    :items=\"users\"\n    :items-per-page=\"5\"\n    class=\"elevation-1\"\n  >\n  <template v-slot:top>\n      <v-toolbar\n        flat\n      >\n        <v-toolbar-title>Data User</v-toolbar-title>\n        <v-divider\n          class=\"mx-4\"\n          inset\n          vertical\n        ></v-divider>\n        <v-spacer></v-spacer>\n        \n      </v-toolbar>\n    </template>\n    <template v-slot:[`item.actions`]=\"{ item }\">\n      <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(item)\"\n      >\n        mdi-pencil\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        mdi-delete\n      </v-icon>\n    </template>\n    <template v-slot:no-data>\n      <v-btn\n        color=\"primary\"\n        @click=\"initialize\"\n      >\n        Reset\n      </v-btn>\n    </template>\n  </v-data-table>\n  </v-container>\n</div>\n</template>\n\n<script>\nimport axios from \"axios\"\nimport Navbar from \"../components/Navbar.vue\";\nexport default {\n  components: { Navbar },\n    mounted: function () {\n      var token = localStorage.getItem(\"token\");\n      axios({\n          method: \"get\",\n          url: \"http://127.0.0.1:8000/api/user\",\n          headers: {\n              \"Authorization\": \"Bearer \" + token\n          }\n      }).then((response) => {\n          console.log(response.data.data);\n          if (response.status == 200 || response.status == 400) {\n              if (response.data.status == true) {\n                  console.log(response.data);\n                  this.users = response.data.data;\n              }\n          }\n      }).catch((err) => {\n        alert('error');\n        let self = this;\n        this.$router.push({\n          name: 'Login',\n        })\n      });\n    },\n    data() {\n        return {\n            dialog: false,\n            dialogDelete: false,\n            headers: [\n                {\n                    align: \"start\",\n                    sortable: false,\n                },\n                { text: \"Nama\", value: \"name\" },\n                { text: \"Username\", value: \"username\" },\n                { text: \"Email\", value: \"email\" },\n                { text: \"Fakultas\", value: \"fakultas\" },\n                { text: \"Jurusan\", value: \"jurusan\" },\n                { text: \"Alamat\", value: \"alamat\" },\n                { text: \"Actions\", value: \"actions\", sortable: false }\n            ],\n            users: [],\n            editedIndex: -1,\n            editedItem: {\n                name: \"\",\n                username: \"\",\n                email: \"\",\n                fakultas: \"\",\n                jurusan: \"\",\n                alamat: \"\",\n            },\n            defaultItem: {\n                name: \"\",\n                username: \"\",\n                email: \"\",\n                fakultas: \"\",\n                jurusan: \"\",\n                alamat: \"\",\n            },\n        };\n    },\n    created() {\n        this.initialize();\n    },\n    methods: {\n      editItem(item) {\n          let self = this;\n          self.$router.push({\n            name: 'Useredit',\n            params: { id: item.id }\n          })\n      },\n      deleteItem(item) {\n        if (confirm(\"Apakah anda yakin ingin menghapus data tersebut?\") == true) {\n          var token = localStorage.getItem(\"token\");\n          let self = this;\n          axios({\n            method: 'delete',\n            url: 'http://localhost:8000/api/user/delete/'+item.id,\n            headers: {\n                \"Authorization\": \"Bearer \" + token\n            }\n          }).then(function (response) {\n            if (response.status == 200) {\n              if (response.data.status == true) {\n                alert(response.data.message);\n                location.reload();\n              }\n              else {\n                alert(response.data.message);\n              }\n            }\n          }).catch((err) => {\n            alert('error');\n            let self = this;\n            this.$router.push({\n              name: 'Login',\n            })\n          });\n        }\n      },\n    },\n}\n</script>"]}]}